{
  "name": "iphone",
  "private": true,
  "description": "iPhone",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.4.13",
    "browser-sync": "2.27.10",
    "del": "^7.0.0",
    "gh-pages": "^5.0.0",
    "gulp": "4.0.2",
    "gulp-htmlmin": "5.0.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-libsquoosh": "^1.0.12",
    "gulp-imagemin": "^8.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "2.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "gulp-terser": "2.1.0",
    "gulp-webp": "^4.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^9.0.2",
    "postcss": "8.4.14",
    "postcss-csso": "^6.0.1",
    "stylelint": "14.15.0",
    "stylelint-config-htmlacademy": " 1.1.4"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "lint": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp",
    "deploy": "gulp build && gh-pages -d build"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "^16"
  },
  "type": "module"
}
